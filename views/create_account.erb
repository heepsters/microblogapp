<form action="/create_account" method="POST">
	<div class="freesignup">
		<img src="images/dclogo.jpg"><h2>Free Sign Up</h2>
		<input type="firstname" name="first" placeholder="First name">
		<input type="lastname" name="last" placeholder="Last name">
		<input type="email" name="email" placeholder="Type your email here">
		<input type="password" name="password" placeholder="Create password">
		<input type="password" name="confirm" placeholder="Confirm password">
		<input id="signupbtn" type="submit" name="Sign Up!" value="Enter developerscloud.com">
	</div>
</form>